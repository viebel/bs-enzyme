{
  "name": "bs-enzyme",
  "version": "0.2.0",
  "description": "Bucklescript bindings for Enzyme",
  "scripts": {
    "test": "jest --watch",
    "build": "bsb -make-world",
    "watch": "bsb -make-world -w",
    "clean": "bsb -clean-world"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpowelll/bs-enzyme.git"
  },
  "keywords": [
    "bucklescript",
    "enzyme",
    "testing"
  ],
  "author": "Rhys Powell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpowelll/bs-enzyme/issues"
  },
  "homepage": "https://github.com/rpowelll/bs-enzyme#readme",
  "devDependencies": {
    "bs-jest": "^0.1.0",
    "bs-platform": "^1.9.1",
    "jest": "^20.0.4",
    "react": "^16.1.1",
    "reason-react": "^0.2.3"
  },
  "dependencies": {
    "enzyme": "^3.2.0",
    "react-dom": "^16.1.1",
    "react-test-renderer": "^16.1.1"
  },
  "peerDependencies": {
    "reason-react": ">=0.2.1",
    "bs-platform": ">=1.9.0"
  },
  "jest": {
    "testMatch": [
      "**/src/**/*_test.js"
    ]
  }
